<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.micol.ex02.member.service.member-map">
	<select id="memberListSelect"
		resultType="co.micol.ex02.member.vo.MemberVO">
		select * from member
	</select>
	
	<select id = "memberSelect" parameterType="co.micol.ex02.member.vo.MemberVO" resultType="co.micol.ex02.member.vo.MemberVO">
		select * from member where id = #{id}
	</select>
	
	<insert id="memberInsert" parameterType="co.micol.ex02.member.vo.MemberVO" >
		insert into member values(#{id},#{password},#{name},#{tel})
	</insert>
</mapper>